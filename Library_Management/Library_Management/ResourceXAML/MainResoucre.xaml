    <viewmodelBorrow:ReceiveBookViewModel x:Key="ReceiveBookVM"></viewmodelBorrow:ReceiveBookViewModel>

    <viewmodelHuman:HumanViewModel x:Key="HumanVM"></viewmodelHuman:HumanViewModel>
    <viewmodelHuman:AddHumanViewModel x:Key="AddHumanVM"></viewmodelHuman:AddHumanViewModel>
