    <viewmodelHuman:ChangePassowrdUserHumanViewModel x:Key="ChangePassUserHumanVM"></viewmodelHuman:ChangePassowrdUserHumanViewModel>

    <viewmodelStaff:TimeTableViewModel x:Key="TimeTableVM"></viewmodelStaff:TimeTableViewModel>
    <viewmodelStaff:UserStaffViewModel x:Key="UserStaffVM"></viewmodelStaff:UserStaffViewModel>
    <viewmodelStaff:AddUserStaffViewModel x:Key="AddUserStaffVM"></viewmodelStaff:AddUserStaffViewModel>
    <viewmodelStaff:EditUserStaffViewModel x:Key="EditUserStaffVM"></viewmodelStaff:EditUserStaffViewModel>
    <viewmodelStaff:ChangePassUserStaffViewModel x:Key="ChangePassUserStaffVM"></viewmodelStaff:ChangePassUserStaffViewModel>

    <viewmodelAdmin:StatisticViewModel x:Key="StatisticVM"></viewmodelAdmin:StatisticViewModel>
    <viewmodelAdmin:BillViewModel x:Key="BillVM"></viewmodelAdmin:BillViewModel>

    <viewmodelUserControl:MyFilesViewModel x:Key="MyFilesVM"></viewmodelUserControl:MyFilesViewModel> 

</ResourceDictionary>